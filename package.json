{
  "name": "educational-recommender",
  "version": "1.0.0",
  "description": "An educational recommender system with AI integration",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && python run.py",
    "start:frontend": "cd frontend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && pip install -r requirements.txt"
  },
  "dependencies": {
    "concurrently": "^7.0.0",
    "react-router-dom": "^6.20.0",
    "react": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-dom": "^18.2.0"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version"
    ],
    "production": [
      ">0.2%",
      "not dead"
    ]
  }
}
